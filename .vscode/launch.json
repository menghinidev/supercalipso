    "version": "0.2.0",
    "configurations": [
        {
            "name": "supercalipso",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
                "--dart-define",
                "USE_MOCKED_DATA_SOURCE=true"
            ]
        },
        {
            "name": "supercalipso (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "toolArgs": [
                "--dart-define",
                "USE_MOCKED_DATA_SOURCE=false"
            ]
        },
        {
            "name": "supercalipso (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "toolArgs": [
                "--dart-define",
                "USE_MOCKED_DATA_SOURCE=false"
            ]
        }
    ]
}